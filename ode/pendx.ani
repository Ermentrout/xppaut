# animation file for pendulum
# speed 50
line .5;.5;.5+.4*sin(x);.5-.4*cos(x);$BLACK;3
fcircle .5+.4*sin(x);.5-.4*cos(x);.05;$RED
# grab the ball of the pendulum and then release with a swing!!
grab .5+.4*sin(x);.5-.4*cos(x);.02
{x=th(mouse_x,mouse_y)}
{x=th(mouse_x,mouse_y);xp=thdot(mouse_x,mouse_y,mouse_vx,mouse_vy);runnow=1}
end





















